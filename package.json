{
  "name": "ai-sdk-cost-calculator",
  "version": "0.1.5",
  "description": "Cost calculator for Vercel AI SDK token usage. Supports OpenAI, Anthropic, Google, xAI (Grok), and DeepSeek with long context pricing, prompt caching, and reasoning tokens.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "vercel-ai",
    "cost",
    "calculator",
    "pricing",
    "token",
    "usage",
    "openai",
    "anthropic",
    "claude",
    "google",
    "gemini",
    "xai",
    "grok",
    "deepseek",
    "llm",
    "gpt",
    "chatgpt"
  ],
  "author": "Ren Koya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RenKoya1/ai-sdk-cost-calculator.git"
  },
  "bugs": {
    "url": "https://github.com/RenKoya1/ai-sdk-cost-calculator/issues"
  },
  "homepage": "https://github.com/RenKoya1/ai-sdk-cost-calculator#readme",
  "peerDependencies": {
    "ai": ">=6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ai": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}